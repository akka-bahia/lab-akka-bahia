# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                           controllers.Application.index()
GET         /blank                      controllers.Application.blank()

GET         /ses/:to                    @controllers.Application.ses(to: String)
GET         /s3                         controllers.Application.s3()

POST        /cache/:keyObj              controllers.Application.setCache(keyObj: String)
GET         /cache/:key                 controllers.Application.getCache(key: String)

POST        /upload-old                 controllers.Application.setFileUp()
POST        /upload                     controllers.Application.upload()

OPTIONS     /upload      				controllers.Application.option()
#OPTIONS    /contact/:id  				controllers.ContactController.optionID(id: Long)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
